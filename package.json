{
  "name": "classed-elements",
  "description": "classed-elements is a utility that simplifies the process of reusing components with Tailwind CSS in SolidJS",
  "version": "0.0.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/classed-elements.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/classed-elements.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "solid-js": "^1.8.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-solid": "^2.7.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8.1"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hungtcs/classed-elements.git"
  },
  "keywords": [
    "solid",
